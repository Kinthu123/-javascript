{
    "redirects": [
      {
        "source": "/old-path",
        "destination": "/new-path",
        "permanent": true
      }
    ],
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/api/functions/:path*"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  